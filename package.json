{
  "name": "steal-can-example",
  "description": "StealJS and CanJS example",
  "version": "0.0.0",
  "author": {
    "name": "Bitovi",
    "email": "contact@bitovi.com",
    "web": "http://bitovi.com/"
  },
  "scripts": {
    "test": "grunt test --stack"
  },
  "dependencies": {
    "can": "^3.6.0",
    "jquery": "^3.2.1"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/mit-license.php"
    }
  ],
  "main": "main.js",
  "devDependencies": {
    "grunt": "^1.0.1",
    "steal-tools": "^1.3.3",
    "steal": "^1.5.2",
    "documentjs": "0.4.4"
  },
  "system": {
    "configDependencies": [
      "live-reload"
    ],
    "npmDependencies": [
      "can",
      "jquery"
    ],
    "bundle": [
      "components/home/home",
      "components/bootstrap/bootstrap",
      "components/login/login",
      "components/jquery-ui/jquery-ui"
    ],
    "paths": {
      "theme/*": "bower_components/jquery-ui/themes/base/jquery.ui.*",
      "ui/*": "bower_components/jquery-ui/ui/jquery.ui.*.js",
      "bootstrap": "bower_components/bootstrap/dist/js/bootstrap.js",
      "bootstrap.css": "bower_components/bootstrap/dist/css/bootstrap.css"
    },
    "meta": {
      "ui/core": {
        "deps": [
          "jquery",
          "theme/core.css!",
          "theme/theme.css!"
        ]
      },
      "ui/widget": {
        "deps": [
          "jquery"
        ]
      },
      "ui/accordion": {
        "deps": [
          "ui/core",
          "ui/widget",
          "theme/accordion.css!"
        ]
      }
    }
  }
}
